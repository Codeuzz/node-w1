extends ./layout

block content
  h1 Ajouter un utilisateur
  form(action="/add-user" method="POST" id="home-form")
    label(for="name") Nom :
    input(type="text" name="name" required)
    br
    label(for="birth") Date de naissance :
    input(type="date" name="birth" required)
    br
    button(type="submit") Ajouter